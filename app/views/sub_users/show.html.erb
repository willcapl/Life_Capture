
<div class="container">
  <%= link_to 'Dashboard', dashboard_path %>
  <h1>Show for <%= @sub_user.name %></h1>
  <%= link_to 'Edit', edit_sub_user_path(@sub_user) %>
  <%= link_to 'Delete Subuser', sub_user_path(@sub_user), data: {turbo_method: :delete, turbo_confirm: 'Are you sure you want to delete this subuser?'} %>
  <div class="show-container">
    <div class="btn-charcol">
      <h3>Create new memory</h3>
      <div class="plus-icon-bg">
        <i class="fas fa-plus plus-icon"></i>
      </div>
    </div>

    <div class="videos-container">
      <h3>You have no memories yet...</h3>
    </div>

  </div>
</div>
